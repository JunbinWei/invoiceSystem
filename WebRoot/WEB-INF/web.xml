<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
  <display-name></display-name>
  <servlet>
    <description>登录页面</description>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>cn.gavid.invoiceSystem.login.web.servlet.LoginServlet</servlet-class>
  </servlet>
  <servlet>
    <description>进项数据显示</description>
    <servlet-name>InputTaxServlet</servlet-name>
    <servlet-class>cn.gavid.invoiceSystem.dataHandler.web.servlet.InputTaxServlet</servlet-class>
  </servlet>
  <servlet>
    <description>销项数据显示</description>
    <servlet-name>OutputTaxServlet</servlet-name>
    <servlet-class>cn.gavid.invoiceSystem.dataHandler.web.servlet.OutputTaxServlet</servlet-class>
  </servlet>
  <servlet>
    <description>进销项对比分析</description>
    <servlet-name>ContrastServlet</servlet-name>
    <servlet-class>cn.gavid.invoiceSystem.dataHandler.web.servlet.ContrastServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>PageServlet</servlet-name>
    <servlet-class>cn.gavid.invoiceSystem.dataHandler.web.servlet.PageServlet</servlet-class>
  </servlet>






  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>InputTaxServlet</servlet-name>
    <url-pattern>/InputTaxServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>OutputTaxServlet</servlet-name>
    <url-pattern>/OutputTaxServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ContrastServlet</servlet-name>
    <url-pattern>/ContrastServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>PageServlet</servlet-name>
    <url-pattern>/PageServlet</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>login.jsp</welcome-file>
  </welcome-file-list>
</web-app>
